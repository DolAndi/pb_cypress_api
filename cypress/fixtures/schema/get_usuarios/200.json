{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
        "quantidade",
        "usuarios"
    ],
    "properties": {
        "quantidade": {
            "type": "integer"
        },
        "usuarios": {
            "type": "array",
            "additionalItems": true,
            "items": {
                "anyOf": [
                    {
                        "type": "object",
                        "required": [
                            "administrador",
                            "email",
                            "nome",
                            "password",
                            "_id"
                        ],
                        "properties": {
                            "administrador": {
                                "type": "string"
                            },
                            "email": {
                                "type": "string"
                            },
                            "nome": {
                                "type": "string"
                            },
                            "password": {
                                "type": "string"
                            },
                            "_id": {
                                "type": "string"
                            }
                        },
                        "additionalProperties": true
                    }
                ]
            }
        }
    },
    "additionalProperties": true
}